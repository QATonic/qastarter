<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="API Test Suite" parallel="methods" thread-count="3">
    
    <listeners>
        <listener class-name="{{javaPackage}}.listeners.TestListener"/>
    </listeners>
    
    <test name="User API Tests">
        <classes>
            <class name="{{javaPackage}}.tests.UserTests"/>
        </classes>
    </test>
    
</suite>