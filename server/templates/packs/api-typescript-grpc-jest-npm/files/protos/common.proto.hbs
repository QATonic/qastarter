syntax = "proto3";

package common;

message DeleteResponse {
bool success = 1;
optional string message = 2;
}

message Empty {}

message Timestamp {
int64 seconds = 1;
int32 nanos = 2;
}

message Pagination {
int32 limit = 1;
int32 offset = 2;
int32 total = 3;
}

message ErrorDetail {
string code = 1;
string message = 2;
map<string, string> metadata = 3;
    }