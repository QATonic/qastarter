{
  "id": "api-typescript-grpc-jest-npm",
  "displayName": "TypeScript gRPC API Framework",
  "description": "Professional gRPC API automation framework with TypeScript, @grpc/grpc-js, Jest, and npm",
  "version": "2.0.0",
  "supportsDynamic": true,
  "supportedCombination": {
    "testingType": "api",
    "framework": "grpc",
    "language": "typescript",
    "testRunner": "jest",
    "buildTool": "npm"
  },
  "toolVersions": {
    "node": "18+",
    "grpc-js": "1.9.0",
    "jest": "29.7.0",
    "typescript": "5.3.0"
  },
  "files": [
    {
      "path": "package.json",
      "isTemplate": true,
      "description": "Node.js package configuration"
    },
    {
      "path": "tsconfig.json",
      "isTemplate": true,
      "description": "TypeScript configuration"
    },
    {
      "path": "jest.config.ts",
      "isTemplate": true,
      "description": "Jest configuration"
    },
    {
      "path": "README.md",
      "isTemplate": true,
      "description": "Project documentation"
    },
    {
      "path": ".github/workflows/tests.yml",
      "isTemplate": true,
      "conditional": {
        "cicdTool": "github-actions"
      },
      "description": "GitHub Actions workflow"
    },
    {
      "path": "Jenkinsfile",
      "isTemplate": true,
      "conditional": {
        "cicdTool": "jenkins"
      },
      "description": "Jenkins pipeline"
    },
    {
      "path": "azure-pipelines.yml",
      "isTemplate": true,
      "conditional": {
        "cicdTool": "azure-devops"
      },
      "description": "Azure DevOps pipeline"
    },
    {
      "path": ".gitlab-ci.yml",
      "isTemplate": true,
      "conditional": {
        "cicdTool": "gitlab-ci"
      },
      "description": "GitLab CI pipeline"
    },
    {
      "path": "Dockerfile",
      "isTemplate": true,
      "conditional": {
        "utilities.includeDocker": true
      },
      "description": "Docker configuration"
    },
    {
      "path": "docker-compose.yml",
      "isTemplate": true,
      "conditional": {
        "utilities.includeDockerCompose": true
      },
      "description": "Docker Compose configuration"
    },
    {
      "path": "allure.properties",
      "isTemplate": true,
      "conditional": {
        "reportingTool": "allure"
      },
      "description": "Allure configuration"
    },
    {
      "path": "config/ConfigReader.ts",
      "isTemplate": true,
      "description": "Configuration reader"
    },
    {
      "path": "core/GrpcClient.ts",
      "isTemplate": true,
      "description": "gRPC client wrapper"
    },
    {
      "path": "src/client/grpc-client.ts",
      "isTemplate": true,
      "description": "gRPC client implementation"
    },
    {
      "path": "src/config/environments.ts",
      "isTemplate": true,
      "description": "Environment configurations"
    },
    {
      "path": "src/types/grpc.types.ts",
      "isTemplate": true,
      "description": "gRPC type definitions"
    },
    {
      "path": "src/services/user-service.ts",
      "isTemplate": true,
      "description": "User gRPC service client"
    },
    {
      "path": "src/utils/test-helpers.ts",
      "isTemplate": true,
      "description": "Test helper utilities"
    },
    {
      "path": "protos/user.proto",
      "isTemplate": true,
      "description": "User protobuf definition"
    },
    {
      "path": "protos/common.proto",
      "isTemplate": true,
      "description": "Common protobuf definitions"
    },
    {
      "path": "utils/Logger.ts",
      "isTemplate": true,
      "description": "Logging utility"
    },
    {
      "path": "tests/setup.ts",
      "isTemplate": true,
      "description": "Jest test setup"
    },
    {
      "path": "tests/GrpcTests.spec.ts",
      "isTemplate": true,
      "description": "gRPC API tests"
    },
    {
      "path": "tests/user-service.test.ts",
      "isTemplate": true,
      "description": "User service tests"
    },
    {
      "path": "tests/streaming.test.ts",
      "isTemplate": true,
      "description": "gRPC streaming tests"
    },
    {
      "path": "tests/error-handling.test.ts",
      "isTemplate": true,
      "description": "Error handling tests"
    }
  ],
  "directories": [
    "logs",
    "reports",
    "protos"
  ],
  "dynamicSupport": {
    "reportingTools": [
      "jest-html-reporter",
      "allure"
    ],
    "cicdTools": [
      "jenkins",
      "github-actions",
      "gitlab-ci",
      "azure-devops"
    ],
    "testingPatterns": [
      "contract-testing"
    ]
  }
}