<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- Report theme - standard or dark -->
        <theme>standard</theme>
        
        <!-- Document encoding -->
        <encoding>UTF-8</encoding>
        
        <!-- Protocol for report resources (http or https) -->
        <protocol>https</protocol>
        
        <!-- Report title -->
        <documentTitle>{{projectName}} - Test Report</documentTitle>
        
        <!-- Report name -->
        <reportName>{{projectName}} Playwright Automation Report</reportName>
        
        <!-- Report headline -->
        <reportHeadline>Automated Test Execution Results</reportHeadline>
        
        <!-- Chart location - top or bottom -->
        <chartLocation>top</chartLocation>
        
        <!-- Enable/disable timeline -->
        <enableTimeline>true</enableTimeline>
        
        <!-- Date/time format -->
        <timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>
        
        <!-- Custom CSS (optional) -->
        <css></css>
        
        <!-- Custom JavaScript (optional) -->
        <scripts></scripts>
    </configuration>
</extentreports>
