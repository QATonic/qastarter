<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="{{projectName}} Test Suite" parallel="methods" thread-count="2">

    <listeners>
        <listener class-name="{{javaPackage}}.listeners.TestListener" />
    </listeners>

    <test name="Login Tests">
        <classes>
            <class name="{{javaPackage}}.tests.LoginTests" />
        </classes>
    </test>

</suite>