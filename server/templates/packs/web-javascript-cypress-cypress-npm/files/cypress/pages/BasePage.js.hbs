class BasePage {
  visit(path = "/") { cy.visit(path); return this; }
  click(selector) { cy.get(selector).click(); return this; }
  type(selector, text) { cy.get(selector).clear().type(text); return this; }
  shouldBeVisible(selector) { cy.get(selector).should("be.visible"); return this; }
}
module.exports = { BasePage };