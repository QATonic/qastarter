{
  "id": "web-javascript-cypress-cypress-npm",
  "displayName": "JavaScript Cypress Framework",
  "description": "Production-ready Cypress automation framework with JavaScript",
  "version": "2.0.0",
  "supportsDynamic": true,
  "supportedCombination": {
    "testingType": "web",
    "framework": "cypress",
    "language": "javascript",
    "testRunner": "cypress",
    "buildTool": "npm"
  },
  "dynamicSupport": {
    "reportingTools": [
      "mochawesome"
    ],
    "cicdTools": [
      "jenkins",
      "github-actions",
      "gitlab-ci",
      "azure-devops",
      "circleci"
    ],
    "testingPatterns": [
      "page-object-model",
      "bdd"
    ]
  },
  "toolVersions": {
    "node": "18+",
    "cypress": "13.6.0"
  },
  "files": [
    {
      "path": "package.json",
      "isTemplate": true
    },
    {
      "path": "cypress.config.js",
      "isTemplate": true
    },
    {
      "path": ".prettierrc",
      "isTemplate": true
    },
    {
      "path": "README.md",
      "isTemplate": true
    },
    {
      "path": ".gitignore",
      "isTemplate": true
    },
    {
      "path": "cypress/pages/BasePage.js",
      "isTemplate": true
    },
    {
      "path": "cypress/pages/LoginPage.js",
      "isTemplate": true
    },
    {
      "path": "cypress/e2e/login.cy.js",
      "isTemplate": true
    },
    {
      "path": "cypress/support/e2e.js",
      "isTemplate": true
    },
    {
      "path": ".circleci/config.yml",
      "isTemplate": true,
      "conditional": {
        "cicdTool": "circleci"
      }
    },
    {
      "path": ".github/workflows/tests.yml",
      "isTemplate": true,
      "conditional": {
        "cicdTool": "github-actions"
      }
    },
    {
      "path": ".gitlab-ci.yml",
      "isTemplate": true,
      "conditional": {
        "cicdTool": "gitlab-ci"
      }
    },
    {
      "path": "azure-pipelines.yml",
      "isTemplate": true,
      "conditional": {
        "cicdTool": "azure-devops"
      }
    },
    {
      "path": "cypress/e2e/features/login.feature",
      "isTemplate": true,
      "conditional": {
        "testingPattern": "bdd"
      }
    },
    {
      "path": "cypress/fixtures/config.json",
      "isTemplate": true
    },
    {
      "path": "cypress/fixtures/environments.json",
      "isTemplate": true
    },
    {
      "path": "cypress/fixtures/users.json",
      "isTemplate": true,
      "conditional": {
        "utilities.dataProvider": true
      }
    },
    {
      "path": "cypress/pages/HomePage.js",
      "isTemplate": true
    },
    {
      "path": "cypress/support/commands.js",
      "isTemplate": true
    },
    {
      "path": "cypress/support/step_definitions/loginSteps.js",
      "isTemplate": true,
      "conditional": {
        "testingPattern": "bdd"
      }
    },
    {
      "path": "cypress/support/utils.js",
      "isTemplate": true
    },
    {
      "path": "docker-compose.yml",
      "isTemplate": true,
      "conditional": {
        "utilities.includeDockerCompose": true
      }
    },
    {
      "path": "Dockerfile",
      "isTemplate": true,
      "conditional": {
        "utilities.includeDocker": true
      }
    },
    {
      "path": "Jenkinsfile",
      "isTemplate": true,
      "conditional": {
        "cicdTool": "jenkins"
      }
    }
  ],
  "directories": [
    "cypress/screenshots",
    "cypress/videos",
    "reports"
  ]
}