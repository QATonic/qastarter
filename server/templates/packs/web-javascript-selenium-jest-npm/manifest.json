{
  "id": "web-javascript-selenium-jest-npm",
  "displayName": "JavaScript Selenium Jest Framework",
  "description": "Production-ready Selenium automation framework with JavaScript and Jest",
  "version": "2.0.0",
  "supportsDynamic": true,
  "supportedCombination": {
    "testingType": "web",
    "framework": "selenium",
    "language": "javascript",
    "testRunner": "jest",
    "buildTool": "npm"
  },
  "dynamicSupport": {
    "reportingTools": ["allure", "jest-html"],
    "cicdTools": ["jenkins", "github-actions", "gitlab-ci", "azure-devops", "circleci"],
    "testingPatterns": ["page-object-model", "bdd"]
  },
  "toolVersions": {
    "node": "18+",
    "selenium": "4.16.0",
    "jest": "29.7.0"
  },
  "files": [
    {"path": "package.json", "isTemplate": true},
    {"path": "jest.config.js", "isTemplate": true},
    {"path": "README.md", "isTemplate": true},
    {"path": ".gitignore", "isTemplate": false},
    {"path": "config/ConfigReader.js", "isTemplate": true},
    {"path": "config/dev.json", "isTemplate": false},
    {"path": "pages/BasePage.js", "isTemplate": true},
    {"path": "pages/LoginPage.js", "isTemplate": true},
    {"path": "utils/DriverManager.js", "isTemplate": true},
    {"path": "tests/login.spec.js", "isTemplate": true}
  ],
  "directories": ["logs", "screenshots", "reports"]
}