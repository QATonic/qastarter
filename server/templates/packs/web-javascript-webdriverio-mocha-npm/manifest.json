{
  "id": "web-javascript-webdriverio-mocha-npm",
  "displayName": "WebdriverIO JavaScript Framework",
  "supportsDynamic": true,
  "name": "WebdriverIO JavaScript Framework",
  "description": "Modern web automation framework with WebdriverIO, Mocha, and JavaScript",
  "tags": [
    "web",
    "webdriverio",
    "mocha",
    "javascript",
    "e2e",
    "pom"
  ],
  "toolVersions": {
    "node": "18+",
    "webdriverio": "8.27.0",
    "mocha": "8.27.0"
  },
  "files": [
    {
      "path": "package.json",
      "isTemplate": true,
      "description": "NPM package configuration"
    },
    {
      "path": "wdio.conf.js",
      "isTemplate": true,
      "description": "WebdriverIO configuration entry"
    },
    {
      "path": "config/wdio.config.js",
      "isTemplate": true,
      "description": "WebdriverIO main configuration"
    },
    {
      "path": "config/environments.js",
      "isTemplate": true,
      "description": "Environment configuration"
    },
    {
      "path": ".gitignore",
      "isTemplate": true,
      "description": "Git ignore patterns"
    },
    {
      "path": "README.md",
      "isTemplate": true,
      "description": "Project documentation"
    },
    {
      "path": ".github/workflows/tests.yml",
      "isTemplate": true,
      "description": "GitHub Actions CI/CD",
      "conditional": {
        "cicdTool": "github-actions"
      }
    },
    {
      "path": "Jenkinsfile",
      "isTemplate": true,
      "description": "Jenkins CI/CD pipeline",
      "conditional": {
        "cicdTool": "jenkins"
      }
    },
    {
      "path": "azure-pipelines.yml",
      "isTemplate": true,
      "description": "Azure DevOps CI/CD pipeline",
      "conditional": {
        "cicdTool": "azure-devops"
      }
    },
    {
      "path": ".gitlab-ci.yml",
      "isTemplate": true,
      "description": "GitLab CI/CD pipeline",
      "conditional": {
        "cicdTool": "gitlab-ci"
      }
    },
    {
      "path": ".circleci/config.yml",
      "isTemplate": true,
      "description": "CircleCI pipeline configuration",
      "conditional": {
        "cicdTool": "circleci"
      }
    },
    {
      "path": "test/pageobjects/base.page.js",
      "isTemplate": true,
      "description": "Base Page Object class"
    },
    {
      "path": "test/pageobjects/login.page.js",
      "isTemplate": true,
      "description": "Login Page Object"
    },
    {
      "path": "test/pageobjects/home.page.js",
      "isTemplate": true,
      "description": "Home Page Object"
    },
    {
      "path": "test/specs/login.spec.js",
      "isTemplate": true,
      "description": "Login test specifications"
    },
    {
      "path": "test/data/users.js",
      "isTemplate": true,
      "description": "Test data for users"
    },
    {
      "path": "utils/logger.js",
      "isTemplate": true,
      "description": "Logging utility"
    },
    {
      "path": "utils/waitHelpers.js",
      "isTemplate": true,
      "description": "Wait helper utilities"
    },
    {
      "path": "utils/screenshotHelper.js",
      "isTemplate": true,
      "description": "Screenshot capture utility"
    },
    {
      "path": "utils/dataReader.js",
      "isTemplate": true,
      "description": "Test data reader utility"
    },
    {
      "path": "features/login.feature",
      "isTemplate": true,
      "description": "Sample login feature",
      "conditional": {
        "testingPattern": "bdd"
      }
    },
    {
      "path": "step-definitions/loginSteps.js",
      "isTemplate": true,
      "description": "Cucumber-js step definitions",
      "conditional": {
        "testingPattern": "bdd"
      }
    },
    {
      "path": "Dockerfile",
      "isTemplate": true,
      "description": "Docker configuration for containerized testing",
      "conditional": {
        "utilities.includeDocker": true
      }
    },
    {
      "path": "docker-compose.yml",
      "isTemplate": true,
      "description": "Docker Compose for Selenium Grid",
      "conditional": {
        "utilities.includeDockerCompose": true
      }
    }
  ],
  "supportedCombination": {
    "testingType": "web",
    "framework": "webdriverio",
    "language": "javascript",
    "testRunner": "mocha",
    "buildTool": "npm"
  },
  "dynamicSupport": {
    "reportingTools": [
      "allure",
      "jest-html-reporter",
      "mochawesome"
    ],
    "cicdTools": [
      "jenkins",
      "github-actions",
      "gitlab-ci",
      "azure-devops",
      "circleci"
    ],
    "testingPatterns": [
      "page-object-model",
      "bdd"
    ]
  }
}
