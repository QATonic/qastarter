{
  "id": "web-typescript-cypress-cypress-npm",
  "displayName": "TypeScript Cypress Framework",
  "description": "Production-ready Cypress automation framework with TypeScript",
  "version": "2.0.0",
  "supportsDynamic": true,
  "supportedCombination": {
    "testingType": "web",
    "framework": "cypress",
    "language": "typescript",
    "testRunner": "cypress",
    "buildTool": "npm"
  },
  "dynamicSupport": {
    "reportingTools": [
      "allure",
      "mochawesome"
    ],
    "cicdTools": [
      "jenkins",
      "github-actions",
      "gitlab-ci",
      "azure-devops",
      "circleci"
    ],
    "testingPatterns": [
      "page-object-model",
      "bdd"
    ]
  },
  "toolVersions": {
    "node": "18+",
    "cypress": "13.6.0",
    "typescript": "5.3.0"
  },
  "files": [
    {
      "path": "package.json",
      "isTemplate": true
    },
    {
      "path": "tsconfig.json",
      "isTemplate": true
    },
    {
      "path": "cypress.config.ts",
      "isTemplate": true
    },
    {
      "path": "README.md",
      "isTemplate": true
    },
    {
      "path": ".gitignore",
      "isTemplate": true
    },
    {
      "path": "cypress/pages/BasePage.ts",
      "isTemplate": true
    },
    {
      "path": "cypress/pages/LoginPage.ts",
      "isTemplate": true
    },
    {
      "path": "cypress/e2e/login.cy.ts",
      "isTemplate": true
    },
    {
      "path": "cypress/support/e2e.ts",
      "isTemplate": true
    }
  ],
  "directories": [
    "cypress/screenshots",
    "cypress/videos",
    "reports"
  ]
}
