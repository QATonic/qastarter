{
  "id": "web-typescript-playwright-jest-npm",
  "name": "Web Automation Framework",
  "description": "Playwright test automation framework with TypeScript, Jest, and npm for web testing",
  "tags": ["web", "playwright", "typescript", "jest", "npm", "pom", "bdd"],
  "supportsDynamic": true,
  "supportedCombination": {
    "testingType": "web",
    "framework": "playwright",
    "language": "typescript",
    "testRunner": "jest",
    "buildTool": "npm"
  },
  "dynamicSupport": {
    "reportingTools": ["jest-html", "allure", "junit"],
    "cicdTools": ["github-actions", "gitlab-ci", "azure-devops", "circleci", "jenkins"],
    "testingPatterns": ["page-object-model", "bdd"]
  },
  "toolVersions": {
    "node": "20",
    "playwright": "1.41.0",
    "jest": "29.7.0",
    "jest-playwright-preset": "3.0.1",
    "typescript": "5.3.3",
    "jestHtmlReporter": "3.10.2",
    "allureJest": "2.15.2",
    "allureCommandline": "2.25.0",
    "jestJunit": "16.0.0",
    "cucumber": "10.0.1",
    "jestCucumber": "3.0.1"
  },
  "files": [
    {
      "path": "package.json",
      "isTemplate": true,
      "description": "NPM package configuration with TypeScript and Playwright dependencies"
    },
    {
      "path": "tsconfig.json",
      "isTemplate": true,
      "description": "TypeScript compiler configuration"
    },
    {
      "path": "jest.config.js",
