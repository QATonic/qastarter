{
  "id": "web-typescript-selenium-jest-npm",
  "displayName": "TypeScript Selenium Jest Framework",
  "description": "Production-ready Selenium automation framework with TypeScript and Jest",
  "version": "2.0.0",
  "supportsDynamic": true,
  "supportedCombination": {
    "testingType": "web",
    "framework": "selenium",
    "language": "typescript",
    "testRunner": "jest",
    "buildTool": "npm"
  },
  "dynamicSupport": {
    "reportingTools": ["allure", "jest-html"],
    "cicdTools": ["jenkins", "github-actions", "gitlab-ci", "azure-devops", "circleci"],
    "testingPatterns": ["page-object-model", "bdd"]
  },
  "toolVersions": {
    "node": "18+",
    "selenium": "4.16.0",
    "jest": "29.7.0",
    "typescript": "5.3.0"
  },
  "files": [
    {"path": "package.json", "isTemplate": true},
    {"path": "tsconfig.json", "isTemplate": true},
    {"path": "jest.config.ts", "isTemplate": true},
    {"path": "README.md", "isTemplate": true},
    {"path": ".gitignore", "isTemplate": false},
    {"path": ".env.example", "isTemplate": false},
    {"path": "Jenkinsfile", "isTemplate": true, "conditional": {"cicdTool": "jenkins"}},
    {"path": ".github/workflows/tests.yml", "isTemplate": true, "conditional": {"cicdTool": "github-actions"}},
    {"path": ".gitlab-ci.yml", "isTemplate": true, "conditional": {"cicdTool": "gitlab-ci"}},
    {"path": "azure-pipelines.yml", "isTemplate": true, "conditional": {"cicdTool": "azure-devops"}},
    {"path": ".circleci/config.yml", "isTemplate": true, "conditional": {"cicdTool": "circleci"}},
    {"path": "Dockerfile", "isTemplate": true},
    {"path": "docker-compose.yml", "isTemplate": true},
    {"path": "config/ConfigReader.ts", "isTemplate": true},
    {"path": "config/dev.json", "isTemplate": false},
    {"path": "config/qa.json", "isTemplate": false},
    {"path": "pages/BasePage.ts", "isTemplate": true},
    {"path": "pages/LoginPage.ts", "isTemplate": true},
    {"path": "utils/DriverManager.ts", "isTemplate": true},
    {"path": "tests/login.spec.ts", "isTemplate": true},
    {"path": "features/login.feature", "isTemplate": true, "conditional": {"testingPattern": "bdd"}},
    {"path": "step-definitions/loginSteps.ts", "isTemplate": true, "conditional": {"testingPattern": "bdd"}}
  ],
  "directories": ["logs", "screenshots", "reports"]
}